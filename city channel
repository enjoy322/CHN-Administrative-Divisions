//爬取地级市
	var cityList []model.Division
	ch := make(chan model.Division, 1)
	var wg sync.WaitGroup
	for _, division := range provinceList {
		//单线程
		doc := CrawlCity(baseURL, latest, division)
		if doc == nil {
			fail += 1
			fmt.Println("失败一次")
			failMap["City"] = append(failMap["City"], division.Url)
			continue
		}
		//	协程处理
		go func(ch chan model.Division, doc *html.Node, division2 model.Division, w *sync.WaitGroup) {
			w.Add(1)
			defer w.Done()
			DealCity(ch, doc, division2)
		}(ch, doc, division, &wg)
	}

	if _, ok := failMap["City"]; !ok {
		//	无失败
		WriteToJsonFile("./file", "city.json", cityList)
	}

	//01
	go func() {
		wg.Wait()
		close(ch)
	}()

	for division := range ch {
		cityList = append(cityList, division)
	}

	fmt.Println("cityList:", len(cityList), cityList)